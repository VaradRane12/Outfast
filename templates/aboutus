    <script>
        // Parallax effect for background elements
        window.addEventListener('scroll', function() {
            const scrollY = window.scrollY;
            
            // Parallax for hero section
            document.querySelectorAll('.disaster').forEach((element, index) => {
                const speed = 0.1 * (index + 1);
                element.style.transform = `translateY(${scrollY * speed}px)`;
            });
            
            // Parallax for What We Do section
            document.querySelectorAll('.parallax-item').forEach((element, index) => {
                const speed = 0.05 * (index + 1);
                element.style.transform = `translateY(${scrollY * speed}px)`;
            });
        });

        // 3D card effect for center cards
        document.querySelectorAll('.center-card').forEach(card => {
            card.addEventListener('mousemove', function(e) {
                const rect = this.getBoundingClientRect();
                const x = e.clientX - rect.left;
                const y = e.clientY - rect.top;
                
                const centerX = rect.width / 2;
                const centerY = rect.height / 2;
                
                const angleX = (y - centerY) / 20;
                const angleY = (centerX - x) / 20;
                
                this.style.transform = `rotateX(${angleX}deg) rotateY(${angleY}deg) translateZ(20px)`;
            });
            
            card.addEventListener('mouseleave', function() {
                this.style.transform = 'rotateX(0) rotateY(0) translateZ(0)';
            });
        });

        // Reviews slider functionality with 3D effect
        document.addEventListener('DOMContentLoaded', function() {
            const container = document.querySelector('.reviews-container');
            const reviews = document.querySelectorAll('.review');
            const prevBtn = document.getElementById('prev');
            const nextBtn = document.getElementById('next');
            
            let currentIndex = 0;
            
            function showReview(index) {
                if (index < 0) {
                    currentIndex = reviews.length - 1;
                } else if (index >= reviews.length) {
                    currentIndex = 0;
                } else {
                    currentIndex = index;
                }
                
                container.style.transform = `translateX(-${currentIndex * 100}%)`;
                
                // Add 3D effect to current review
                reviews.forEach((review, i) => {
                    if (i === currentIndex) {
                        review.classList.add('current-review');
                    } else {
                        review.classList.remove('current-review');
                    }
                });
            }
            
            prevBtn.addEventListener('click', () => {
                showReview(currentIndex - 1);
            });
            
            nextBtn.addEventListener('click', () => {
                showReview(currentIndex + 1);
            });
            
            // Initialize first review with 3D effect
            showReview(0);
            
            // Auto-rotate reviews every 5 seconds
            setInterval(() => {
                showReview(currentIndex + 1);
            }, 5000);
        });

        // Create floating 3D disaster objects
        function createFloatingDisasters() {
            const disasterTypes = [
                { name: 'hurricane', emoji: '🌀' },
                { name: 'earthquake', emoji: '⚡' },
                { name: 'flood', emoji: '💧' },
                { name: 'fire', emoji: '🔥' },
                { name: 'tornado', emoji: '🌪️' },
                { name: 'volcano', emoji: '🌋' }
            ];
            
            const container = document.createElement('div');
            container.className = 'floating-disasters';
            container.style.position = 'fixed';
            container.style.top = '0';
            container.style.left = '0';
            container.style.width = '100%';
            container.style.height = '100%';
            container.style.pointerEvents = 'none';
            container.style.zIndex = '-1';
            container.style.opacity = '0.1';
            
            for (let i = 0; i < 15; i++) {
                const disaster = document.createElement('div');
                const randomType = disasterTypes[Math.floor(Math.random() * disasterTypes.length)];
                
                disaster.textContent = randomType.emoji;
                disaster.className = `floating-disaster ${randomType.name}`;
                disaster.style.position = 'absolute';
                disaster.style.fontSize = `${Math.random() * 20 + 10}px`;
                disaster.style.left = `${Math.random() * 100}%`;
                disaster.style.top = `${Math.random() * 100}%`;
                disaster.style.animation = `float ${Math.random() * 10 + 5}s ease-in-out infinite alternate, spin ${Math.random() * 20 + 10}s linear infinite`;
                
                container.appendChild(disaster);
            }
            
            document.body.appendChild(container);
        }
        
        createFloatingDisasters();

        // Smooth scrolling for navigation
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function(e) {
                e.preventDefault();
                document.querySelector(this.getAttribute('href')).scrollIntoView({
                    behavior: 'smooth'
                });
            });
        });

        // 3D rotation effect for capability icons
        document.querySelectorAll('.capability-icon').forEach(icon => {
            icon.addEventListener('mouseover', function() {
                this.style.transform = 'rotateY(180deg)';
            });
            
            icon.addEventListener('mouseout', function() {
                this.style.transform = 'rotateY(0)';
            });
        });
    </script>
</body>
